{% extends 'base.html.twig' %}

{% block title %}Modele{% endblock %}

{% block body %}
<div class="white-box">
    <h1 class="text-center">Information de {{ modele.marque}} - {{ modele.modele }}</h1>
    <h1>Modele</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ modele.id }}</td>
            </tr>
            <tr>
                <th>Modele</th>
                <td>{{ modele.modele }}</td>
            </tr>
            <tr>
                <th>Marque</th>
                <td>{{ modele.marque }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_modele_index') }}">← Retour à la liste</a>

    <a href="{{ path('app_modele_edit', {'id': modele.id}) }}">Modifier</a>

    {{ include('modele/_delete_form.html.twig') }}
</div>
{% endblock %}
